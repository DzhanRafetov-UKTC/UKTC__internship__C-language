#include <stdio.h>
#include <math.h>
#include <limits.h>
//zadacha 5
/*
 

 */
int main(){
    int num;
    printf("Enter number:");
    scanf("%d",&num);
    int flag =0;
    int i=1;
    while(i<=num){
        if(i ==num)
        {
            flag=1;
            break;
        }
        i*=2;
    }
    if(flag){
        printf("%d is power of 2\n",num);
    }
    else{
        printf("%d isnot power of 2\n",num);
    }
    return 0;
        }

