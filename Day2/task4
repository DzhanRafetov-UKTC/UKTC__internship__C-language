#include <stdio.h>
#include <math.h>
#include <limits.h>
//zadacha 4
/*
 

 */

int maxNumber(int* arr ,int n ){
    int max=*arr;
    for (int i=0;i<n ; i++) {
       
        
            if(*(arr+i)> max){
                max=*(arr+i);
            }
        
        
        }
    
    
    
    
    return max;
}


int main()
    {
    int a[5]= {1,2,3,4,10};
    printf("Max=%d",maxNumber(a, 5));
    return 0;
        
}

    
