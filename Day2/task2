#include <stdio.h>
#include <math.h>
#include <limits.h>
//zadacha 2
/*
 

 */

int sumArr(int* arr ,int n ){
    int sum=0;
    for (int i=0;i<n ; i++) {
        sum+=*(arr+i);
    }
    
    
    
    return sum;
}

int main(){
    int a[]= {1,2,3,4,5,6,3};
    int s = sumArr(a,6);
    printf("Sum= %d\n",s);
    return 0;
        
}
        
    
